# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Database URL (optional, dari Supabase Settings > Database)
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Session Configuration
SESSION_SECRET=your-session-secret-here-change-in-production

# StarSender WhatsApp Gateway (Optional)
STARSENDER_API_KEY=your-starsender-api-key

# Pakasir Payment Gateway (Optional)
PAKASIR_SLUG=your-pakasir-slug
PAKASIR_API_KEY=your-pakasir-api-key
PAKASIR_BASE_URL=https://pakasir.zone.id
PAKASIR_REDIRECT_URL=https://yourdomain.com